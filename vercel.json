{
  "rewrites": [
    { "source": "/bare/(.+)", "destination": "/api/bare" },
    { "source": "/bare", "destination": "/api/bare" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    },
    {
      "source": "/uv/uv.sw.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "buildCommand": "npm install",
  "outputDirectory": "public"
}
